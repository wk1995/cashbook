<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="cashbook_tv_trade_list_total">
       <item name="android:gravity">center</item>
       <item name="android:textColor">@color/color_grey_7E797B</item>
       <item name="android:layout_width">wrap_content</item>
       <item name="android:layout_height">@dimen/d30dp</item>
    </style>

    <style name="cashbook_tv_trade_list_item">
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="cashbook_account_list_item_amount_tv">
        <item name="android:gravity">center_vertical|end</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:textColor">@color/common_black_2B2A2A</item>
        <item name="android:textSize">@dimen/s14sp</item>
        <item name="android:paddingEnd">@dimen/d10dp</item>
    </style>

    <style name="cashbook_create_or_edit_account_wallet_et">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColorHint">@color/color_grey_7E797B</item>
        <item name="android:textColor">@color/common_black_2B2A2A</item>
    </style>
</resources>